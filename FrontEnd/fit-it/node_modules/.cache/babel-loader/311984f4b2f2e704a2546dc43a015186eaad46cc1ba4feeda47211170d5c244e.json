{"ast":null,"code":"import { mapState } from \"vuex\";\nimport moment from \"moment\";\nexport default {\n  name: \"DetailChallenge\",\n  data() {\n    return {};\n  },\n  watch: {\n    isJoin() {\n      return this.isJoin;\n    }\n  },\n  computed: {\n    ...mapState([\"challenge\", \"indetailGetMissionList\", \"loginUser\", \"isJoin\"]),\n    startd() {\n      return moment(this.challenge.startDate).format(\"YY/MM/DD\");\n    },\n    endd() {\n      return moment(this.challenge.endDate).format(\"YY/MM/DD\");\n    }\n  },\n  created() {\n    console.log(this.$route.params.challengeNo);\n    this.$store.dispatch(\"getDetailChallenge\", this.$route.params.challengeNo);\n    this.$store.dispatch(\"isJoinOrNotLetCheck\", {\n      challenge: this.challenge,\n      challengeNo: this.$route.params.challengeNo,\n      userNo: this.loginUser.userNo\n    });\n    this.$store.dispatch(\"inDetailGetMissionList\", {\n      challengeNo: this.$route.params.challengeNo\n    });\n  },\n  methods: {\n    joinChallenge(challengeNo, userNo) {\n      //index.js에다가 유저 정보랑 챌린지 no 보내\n      this.$store.dispatch(\"joinChallenge\", {\n        challengeNo,\n        userNo\n      });\n    },\n    notJoinChallenge(challengeNo, userNo) {\n      this.$store.dispatch(\"notJoinChallenge\", {\n        challengeNo,\n        userNo\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAsFA;AACA;AAEA;EACAA;EACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;EACAC;IACAC;IACA;IAEA;MACAC;MACAC;MACAC;IACA;IACA;MACAD;IAEA;EAMA;EAGAE;IACAC;MACA;MACA;QACAH;QACAC;MACA;IACA;IACAG;MACA;QACAJ;QACAC;MACA;IACA;EACA;AACA","names":["name","data","watch","isJoin","computed","startd","endd","created","console","challenge","challengeNo","userNo","methods","joinChallenge","notJoinChallenge"],"sourceRoot":"src/components/challenge","sources":["DetailChallenge.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"row m-4\">\r\n      <!-- <div class=\"col-1\"></div> -->\r\n      <div class=\"col\">\r\n        <div class=\"row\">\r\n          <div class=\"col\" id=\"title\">{{ challenge.challengeName }}</div>\r\n          <div class=\"col-2\">\r\n            <div v-if=\"isJoin === 0\">\r\n              <div>\r\n                <input\r\n                  type=\"checkbox\"\r\n                  class=\"btn-check\"\r\n                  id=\"btn-check\"\r\n                  autocomplete=\"off\"\r\n                />\r\n                <label\r\n                  class=\"btn btn-primary\"\r\n                  for=\"btn-check\"\r\n                  @click=\"\r\n                    joinChallenge(challenge.challengeNo, loginUser.userNo)\r\n                  \"\r\n                  >참여하기</label\r\n                >\r\n              </div>\r\n            </div>\r\n            <div v-else>\r\n              <div>\r\n                <input\r\n                  type=\"checkbox\"\r\n                  class=\"btn-check\"\r\n                  id=\"btn-check\"\r\n                  autocomplete=\"off\"\r\n                />\r\n                <label\r\n                  class=\"btn btn-danger\"\r\n                  for=\"btn-check\"\r\n                  @click=\"\r\n                    notJoinChallenge(challenge.challengeNo, loginUser.userNo)\r\n                  \"\r\n                  >참여취소</label\r\n                >\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"row mt-2\"></div>\r\n        <div class=\"col\" id=\"period\">\r\n          &nbsp;<i class=\"bi bi-calendar-check\"></i> &nbsp; &nbsp;{{ startd }} -\r\n          {{ endd }}\r\n        </div>\r\n      </div>\r\n      <div class=\"row mt-4 ms-1\">\r\n        <pre class=\"p-0\">{{ challenge.challengeContent }}</pre>\r\n      </div>\r\n      <div class=\"row\">\r\n        <!-- {{ challenge.missions }}sssss -->\r\n        <li\r\n          class=\"col m-4\"\r\n          v-for=\"(mission, index) in indetailGetMissionList\"\r\n          :key=\"index\"\r\n        >\r\n          \r\n          <!-- <div class=\"card\" style=\"width: 16rem\">\r\n          <img\r\n            class=\"card-img-top\"\r\n            :src=\"mission.videoThumbnail\"\r\n            alt=\"video-img\"\r\n          />\r\n          <div class=\"card-body\">\r\n            <p class=\"card-text text-truncate\">\r\n              {{ mission.videoTitle }}\r\n            </p>\r\n            \r\n          </div>\r\n        </div> -->\r\n        \r\n\r\n        </li>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-1\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from \"vuex\";\r\nimport moment from \"moment\";\r\n\r\nexport default {\r\n  name: \"DetailChallenge\",\r\n  data() {\r\n    return {};\r\n  },\r\n  watch:{\r\n    isJoin(){\r\n      return this.isJoin;\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState([\"challenge\", \"indetailGetMissionList\", \"loginUser\", \"isJoin\"]),\r\n    startd() {\r\n      return moment(this.challenge.startDate).format(\"YY/MM/DD\");\r\n    },\r\n    endd() {\r\n      return moment(this.challenge.endDate).format(\"YY/MM/DD\");\r\n    },\r\n  },\r\n  created() {\r\n    console.log(this.$route.params.challengeNo);\r\n    this.$store.dispatch(\"getDetailChallenge\", this.$route.params.challengeNo);\r\n\r\n    this.$store.dispatch(\"isJoinOrNotLetCheck\", {\r\n      challenge: this.challenge,\r\n      challengeNo: this.$route.params.challengeNo,\r\n      userNo: this.loginUser.userNo,\r\n    });\r\n    this.$store.dispatch(\"inDetailGetMissionList\", {\r\n      challengeNo: this.$route.params.challengeNo,\r\n\r\n    });\r\n\r\n\r\n\r\n    \r\n\r\n  },\r\n\r\n  \r\n  methods: {\r\n    joinChallenge(challengeNo, userNo) {\r\n      //index.js에다가 유저 정보랑 챌린지 no 보내\r\n      this.$store.dispatch(\"joinChallenge\", {\r\n        challengeNo,\r\n        userNo,\r\n      });\r\n    },\r\n    notJoinChallenge(challengeNo, userNo) {\r\n      this.$store.dispatch(\"notJoinChallenge\", {\r\n        challengeNo,\r\n        userNo,\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#title {\r\n  font-size: 30px;\r\n  color: indigo;\r\n  font-weight: 800;\r\n}\r\n.btn {\r\n  margin: 20px;\r\n}\r\n\r\n pre {\r\n\r\n    width: 100%;\r\n\r\n    white-space: pre-wrap;\r\n\r\n    word-wrap: break-word;\r\n\r\n  }\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}